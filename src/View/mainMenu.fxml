<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@applicationFont.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.mainMenuController">
